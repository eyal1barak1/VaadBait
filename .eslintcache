Users/eyal1barak1/Documents/Web 2020/פרויקט סיום/final-proj-vaad-bait/src/pages/RecipesPage/RecipesPage.js",["110"],"import { useState } from \"react\";\nimport { Button, Col, Container, Row } from \"react-bootstrap\";\nimport { Redirect } from \"react-router-dom\";\nimport NewRecipeModal from \"../../components/NewMessageModal/NewMessageModal\";\nimport RecipeCard from \"../../components/RecipeCard/RecipeCard\";\nimport HoaNavbr from \"../../components/HOANavbar/HOANavbr\";\nimport './RecipesPage.css'\nimport NewMessageModal from \"../../components/NewMessageModal/NewMessageModal\";\n\nfunction RecipesPage(props) {\n    const {activeUser, onLogout, recipes, addMessage} = props;\n    const [showModal, setShowModal] = useState(false);\n\n    if (!activeUser) {\n        return <Redirect to=\"/\"/>\n    }\n\n\n\n    const recipesView = recipes.map(recipe => <Col key={recipe.id} lg={3} md={6}><RecipeCard recipe={recipe}/></Col>)\n\n    return (\n        <div className=\"p-recipes\">\n            <HoaNavbr activeUser={activeUser} onLogout={onLogout}/>\n            <Container>\n                <div className=\"heading\">\n                    <h1>{activeUser.fname}'s Recipes</h1>\n                    <Button variant=\"link\" onClick={() => setShowModal(true)}>New Recipe</Button>\n                </div>\n                <Row>\n                    {recipesView}\n                </Row>\n            </Container>\n            <NewMessageModal show={showModal} handleClose={() => setShowModal(false)} addMessage={addMessage}/>\n        </div>\n    )\n\n}\n\nexport default RecipesPage;",["111","112"],"/Users/eyal1barak1/Documents/Web 2020/פרויקט סיום/final-proj-vaad-bait/src/pages/SignupPage/SignupPage.js",[],"/Users/eyal1barak1/Documents/Web 2020/פרויקט סיום/final-proj-vaad-bait/src/components/HomePageCard/HomePageCard.js",[],"/Users/eyal1barak1/Documents/Web 2020/פרויקט סיום/final-proj-vaad-bait/src/components/HOANavbar/HOANavbr.js",[],"/Users/eyal1barak1/Documents/Web 2020/פרויק